<?xml version="1.0"?>
<extension key="uk.mjco.sexualityselfidentify" type="module">
  <file>sexualityselfidentify</file>
  <name>Sexuality Self-Identify</name>
  <description>A more inclusive way to input sexuality in CiviCRM.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Mikey O'Toole</author>
    <email>mikey@mjco.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://civicrm.org/extensions/sexuality-self-identify</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018/04/27</releaseDate>
  <version>1.0</version>
  <compatibility>
    <ver>5.0.0</ver>
    <ver>5.0.1</ver>
  </compatibility>
  <comments>There are no configuration options for this extension. Simply install it and users will be able to enter sexuality in an unrestricted way.</comments>
  <civix>
    <namespace>CRM/sexualityselfidentify</namespace>
  </civix>
</extension>
